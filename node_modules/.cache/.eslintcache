 \"Marilyn Monroe\"]];\n\n\nclass randomQuoteMachine extends React.Component {\n  constructor(props) {\n    super(props);\n    this.state = {\n      quote: quotes[0][0],\n      author: quotes[0][1],\n      color: \"rgb(255, 170, 0)\"\n\n    }\n\n    this.handleClick = this.handleClick.bind(this);\n  }\n\n  handleClick() {\n    let ind = Math.floor(Math.random() * quotes.length);\n    this.setState({\n      quote: quotes[ind][0],\n      author: quotes[ind][1],\n    });\n    let random1 = Math.floor(Math.random() * 255);\n    let random2 = Math.floor(Math.random() * 255);\n    let random3 = Math.floor(Math.random() * 255);\n\n    let pageColor = `rgb(${random1},${random2},${random3})`;\n    let hoverColor = `rgba(${random1},${random2},${random3},0.8)`;\n\n    document.documentElement.style.setProperty(\"--page-color\", pageColor);\n    document.documentElement.style.setProperty(\"--hover-color\", hoverColor);\n  }\n\n  render() {\n\n    return (\n\n      <div>\n        <link rel=\"stylesheet\" href=\"https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css\" />\n        <div id=\"container\">\n          <div id=\"\">\n            <div id=\"quoteBox\">\n              <div id=\"text\">\n                <h1 id=\"quote\"><span className=\"fa fa-quote-left\" id=\"quote-icon\"></span> {this.state.quote}</h1>\n                <p id=\"author\">- {this.state.author}</p>\n              </div>\n              <div id=\"box2\">\n                <button className=\"btn\" title=\"post on facebook\" ><a href=\"https://www.facebook.com\" target=\"_blank\" className=\"links\"><i className=\"fa fa-facebook\"></i></a></button>\n                <button className=\"btn\" title=\"post on twitter\"><a href=\"twitter.com/intent/tweet\" className=\"links\" id=\"tweet-quote\" target=\"_blank\"><i className=\"fa fa-twitter\"></i></a></button>\n                <button id=\"new-quote\" onClick={this.handleClick} title=\"generate new quote\">New quote</button>\n              </div>\n            </div>\n            <div id=\"creator\">\n              <p>by pavan</p>\n            </div>\n          </div>\n        </div>\n        <script src=\"https://cdn.freecodecamp.org/testable-projects-fcc/v1/bundle.js\"></script>\n      </div>)\n  }\n}\n\n\nexport default randomQuoteMachine;\n","C:\\Users\\use\\Desktop\\FreeCodeCamp\\front_end_libraries\\random-quote-machine\\src\\reportWebVitals.js",[],[],{"ruleId":"21","severity":1,"message":"22","line":4,"column":10,"nodeType":"23","messageId":"24","endLine":4,"endColumn":18},{"ruleId":"25","severity":1,"message":"26","line":71,"column":67,"nodeType":"27","messageId":"28","endLine":71,"endColumn":136,"fix":"29"},"no-unused-vars","'ReactDOM' is defined but never used.","Identifier","unusedVar","react/jsx-no-target-blank","Using target=\"_blank\" without rel=\"noreferrer\" (which implies rel=\"noopener\") is a security risk in older browsers: see https://mathiasbynens.github.io/rel-noopener/#recommendations","JSXOpeningElement","noTargetBlankWithoutNoreferrer",{"range":"30","text":"31"},[4131,4131]," rel=\"noreferrer\""]